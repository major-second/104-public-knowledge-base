// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+shift+alt+h",
        "command": "git.viewHistory"
    },
    {
        "key": "ctrl+shift+alt+t",
        "command": "python.launchTensorBoard"
    },
    {
        "key": "ctrl+k u",
        "command": "-workbench.action.closeUnmodifiedEditors"
    },
    {
        "key": "ctrl+shift+alt+v",
        "command": "markdown-preview-enhanced.openPreviewToTheSide",
        "when": "editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+k v",
        "command": "-markdown-preview-enhanced.openPreviewToTheSide",
        "when": "editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+k v",
        "command": "-markdown.extension.closePreview",
        "when": "markdownPreviewFocus"
    },
    {
        "key": "ctrl+k v",
        "command": "-markdown.showPreviewToSide",
        "when": "!notebookEditorFocused && editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+shift+alt+w",
        "command": "workbench.action.closeEditorsInGroup"
    },
    {
        "key": "ctrl+k w",
        "command": "-workbench.action.closeEditorsInGroup"
    },
    {
        "key": "ctrl+shift+alt+o",
        "command": "workbench.action.closeOtherEditors"
    },
    {
        "key": "ctrl+shift+alt+j",
        "command": "workbench.action.openSettingsJson"
    },
    {
        "key": "ctrl+shift+alt+b",
        "command": "browser.open"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.reloadWindow",
        "when": "isDevelopment"
    },
    {
        "key": "ctrl+shift+alt+r",
        "command": "workbench.action.reloadWindow"
    },
    {
        "key": "ctrl+shift+alt+c",
        "command": "workbench.action.remote.close"
    },
    {
        "key": "ctrl+shift+alt+p",
        "command": "gitProjectManager.openProject"
    },
    {
        "key": "ctrl+alt+p",
        "command": "-gitProjectManager.openProject"
    },
    {
        "key": "ctrl+shift+alt+0",
        "command": "gitProjectManager.refreshProjects"
    },
    {
        "key": "ctrl+shift+alt+s",
        "command": "workbench.userDataSync.actions.syncNow"
    }
]